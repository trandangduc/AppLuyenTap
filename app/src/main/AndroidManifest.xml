<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppLuyenTap"
        tools:targetApi="31">

        <!-- Main Entry Point of the Application -->
        <activity
            android:name=".MainActivity"
            android:exported="true">  <!-- Phải export để có thể truy cập từ bên ngoài -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".homepage"
            android:exported="true" />  <!-- Không cho phép truy cập từ bên ngoài -->

        <activity
            android:name=".Clickbody"
            android:exported="true" />  <!-- Cho phép truy cập từ bên ngoài -->

        <activity
            android:name=".Targetpage"
            android:exported="false" />  <!-- Không cho phép truy cập từ bên ngoài -->

        <activity
            android:name=".Levelofmovement"
            android:exported="true" />  <!-- Cho phép truy cập từ bên ngoài -->

        <activity
            android:name=".Gender"
            android:exported="true" />  <!-- Cho phép truy cập từ bên ngoài -->

        <activity
            android:name=".Impulse"
            android:exported="true" />  <!-- Cho phép truy cập từ bên ngoài -->

    </application>

</manifest>
